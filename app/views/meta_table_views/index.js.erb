$(".meta_wrapper > .top_header_wrapper").html("<%= escape_javascript(render partial: '/meta_table_views/header', locals: { header: MetaTable.header } ) %>");
$(".meta_wrapper > table").html("<%= escape_javascript(render partial: '/meta_table_views/table', locals: { content: MetaTable.content } ) %>");
$(".meta_wrapper .table_footer").html("<%= escape_javascript(render partial: '/meta_table_views/footer', locals: { footer: MetaTable.footer, collection: MetaTable.collection } ) %>");
  

function rebind_change() {
  $('#meta_table_search_form').submit();
}

$('#table_view').change(rebind_change);